{
  "name": "amazona-backend",
  "version": "1.0.0",
  "description": "Backend for Amazona (MySQL/Sequelize)",
  "main": "server.js",
  "scripts": {
    "start": "nodemon --watch . --exec babel-node ./server.js",
    "start:prod": "node dist/server.js",
    "build": "rm -rf dist && babel ./ --out-dir dist --ignore ./node_modules,./dist",
    "seed": "node scripts/seed.js"
  },
  "dependencies": {
    "aws-sdk": "^2.702.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.32.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "nodemon": "^2.0.2",
    "eslint": "^6.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/basir/amazona"
  },
  "license": "ISC",
  "engines": {
    "node": "12.4.0",
    "npm": "6.9.0"
  }
}
